{
  "name": "webinar-task-manager-auth",
  "version": "1.0.0",
  "description": "webinar task manager con auth y subida de imagenes",
  "main": "index.js",
  "author": "eduardo de rivero",
  "license": "MIT",
  "scripts": {
    "start:dev": "dotenv -e .env nodemon src/server.ts",
    "build": "tsc -p .",
    "start": "node ./dist/src/server.js",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tsc --project tsconfig.json --noEmit && eslint \"{src,apps,libs,test}/**/*.ts\" --color --max-warnings=0",
    "prisma:migrate:save": "prisma migrate dev --preview-feature --create-only",
    "prisma:migrate:run": "prisma migrate deploy --preview-feature",
    "prisma:migrate:run:test": "dotenv -e .env.test -- prisma migrate deploy --preview-feature",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:migrate:reset:test": "dotenv -e .env.test -- prisma migrate reset --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "prisma:format": "prisma format"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv-cli": "^4.1.1",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.14",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
